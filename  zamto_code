        "def handleRate(value):\n",
        "    if(value== 'NEW' or value =='-'):\n",
        "        return np.nan\n",
        "    else:\n",
        "        value= str(value).split('/')\n",
        "        return float(value[0])\n",
        "df['rate']= df['rate'].apply(handleRate)\n",
        "df['rate'].head()"
