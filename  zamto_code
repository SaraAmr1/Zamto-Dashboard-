        "sizes = df.groupby('book_table')['rate'].mean()\n",
        "labels = sizes.index\n",
        "\n",
        "def custom_autopct(pct, allvals):\n",
        "    absolute = allvals[int(pct/100. * len(allvals))]\n",
        "    return f\"{absolute:.1f}\"\n",
        "\n",
        "fig1, ax1 = plt.subplots()\n",
