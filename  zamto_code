      ],
      "source": [
        "geolocator = Nominatim(user_agent=\"restaurant_locator\")\n",
        "\n",
        "# Function to get latitude and longitude\n",
        "def get_coordinates(location):\n",
        "    try:\n",
        "        loc = geolocator.geocode(location)\n",
        "        if loc:\n",
        "            return loc.latitude, loc.longitude\n",
        "        else:\n",
        "            return None, None\n",
        "    except Exception as e:\n",
        "        print(e)\n",
        "        return None, None\n",
        "\n",
        "# Apply the function to get coordinates for each location\n",
        "location_counts['Coordinates'] = location_counts['Location'].apply(get_coordinates)\n",
        "\n",
        "# Split the coordinates into latitude and longitude\n",
        "location_counts[['Latitude', 'Longitude']] = pd.DataFrame(location_counts['Coordinates'].tolist(), index=location_counts.index)\n",
        "\n",
        "# Remove rows where coordinates couldn't be found\n",
        "location_counts = location_counts.dropna(subset=['Latitude', 'Longitude'])\n",
        "\n",
        "print(location_counts.head())\n"
      ]
