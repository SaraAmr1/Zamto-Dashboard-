      "source": [
        "def handleComma(value):\n",
        "    if ',' in value:\n",
        "        value= value.replace(',' , '')\n",
        "        return float(value)\n",
        "    else:\n",
        "        return float(value)\n",
        "\n",
        "df['Cost2people']= df['Cost2people'].apply(handleComma)\n",
        "df['Cost2people'].head()"
      ]
    },
