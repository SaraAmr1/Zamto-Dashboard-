      "outputs": [
        {
          "output_type": "stream",
          "name": "stderr",
          "text": [
            "<ipython-input-35-a9e11fd74061>:20: FutureWarning: \n",
            "\n",
            "Passing `palette` without assigning `hue` is deprecated and will be removed in v0.14.0. Assign the `x` variable to `hue` and set `legend=False` for the same effect.\n",
            "\n",
            "  sns.barplot(x=cost_range_distribution.index, y=cost_range_distribution.values, palette=colors)\n"
          ]
        },
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 800x600 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "def cost_range_column(row):\n",
        "    if row['Cost2people'] <= 500:\n",
        "        return 'Low'\n",
        "    elif row['Cost2people'] > 500 and row['Cost2people'] <= 1200:\n",
        "        return 'Medium'\n",
        "    elif row['Cost2people'] > 1200 and row['Cost2people'] <= 2500:\n",
        "        return 'High'\n",
        "    elif row['Cost2people'] > 2500 and row['Cost2people'] <= 4000:\n",
        "        return 'Very High'\n",
        "    else:\n",
        "        return 'Luxury'\n",
        "\n",
        "df['cost_range'] = df.apply(cost_range_column, axis=1)\n",
        "\n",
        "cost_range_distribution = df['cost_range'].value_counts()\n",
        "\n",
        "colors = ['#b30000', '#ff4d4d', '#ff1a1a', '#ff9999', '#ffcccc']  # Add an extra shade for 'Luxury'\n",
        "\n",
        "plt.figure(figsize=(8, 6))\n",
        "sns.barplot(x=cost_range_distribution.index, y=cost_range_distribution.values, palette=colors)\n",
        "plt.title('Distribution of Restaurants by Cost Range', fontsize=16)\n",
        "plt.xlabel('Cost Range', fontsize=12)\n",
        "plt.ylabel('Number of Restaurants', fontsize=12)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n"
      ]
    },
